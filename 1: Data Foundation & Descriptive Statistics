       " 'MonthlyCharges': Series([], Name: MonthlyCharges, dtype: float64),\n",
       " 'TotalCharges': Series([], Name: TotalCharges, dtype: float64)}"
      ]
     },
     "execution_count": 8,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def detect_outliers(df):\n",
    "    numeric_df = df.select_dtypes(include=np.number)\n",
    "    outliers = {}\n",
    "    for col in numeric_df.columns:\n",
    "        q1 = numeric_df[col].quantile(0.25)\n",
    "        q3 = numeric_df[col].quantile(0.75)\n",
    "        threshold = 1.5 * (q3 -q1)\n",
    "        outliers[col] = numeric_df[col][(numeric_df[col] < (q1 - threshold)) | (numeric_df[col] > (q3 + threshold))]\n",
    "    return outliers\n",
    "detect_outliers(df_int)\n",
    "        "
   ]
  },
  {
