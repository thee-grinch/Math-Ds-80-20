   "source": [
    "def calc_stats(df):\n",
    "    numeric_df = df.select_dtypes(include=[np.number])\n",
    "    if numeric_df.empty:\n",
    "        print(\"No numeric columns found in the DataFrame\")\n",
    "        return None\n",
    "    stats_df = numeric_df.describe()\n",
    "    stats_df.loc['skewness'] = numeric_df.skew()\n",
    "    stats_df.loc['kurtosis'] = numeric_df.kurtosis()\n",
    "    stats_df.loc['range'] = numeric_df.max() - numeric_df.min()\n",
    "    stats_df.loc['cv'] = numeric_df.std() / numeric_df.mean()  # coefficient of variation\n",
    "    stats_df.loc['iqr'] = numeric_df.quantile(0.75) - numeric_df.quantile(0.25)\n",
    "    print(\"=\" * 60)\n",
    "    print(\"COMPREHENSIVE STATISTICS SUMMARY\")\n",
    "    print(\"=\" * 60)\n",
    "    for column in stats_df.columns:\n",
    "        print(f\"\\n{column.upper()}:\")\n",
    "        print(\"-\" * 30)\n",
    "        for stat in stats_df.index:\n",
    "            value = stats_df.loc[stat, column]\n",
    "            if isinstance(value, (int, np.integer)):\n",
    "                print(f\"  {stat:<12}: {value:>15}\")\n",
    "            else:\n",
    "                print(f\"  {stat:<12}: {value:>15.6f}\")\n",
    "    return stats_df\n",
    "calc_stats(df_int)"
   ]
  },
